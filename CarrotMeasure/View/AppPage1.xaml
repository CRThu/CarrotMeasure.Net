<Page x:Class="CarrotMeasure.View.AppPage1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CarrotMeasure"
      xmlns:hc="https://handyorg.github.io/handycontrol"
      xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
      xmlns:vm="clr-namespace:CarrotMeasure.ViewModel"
      d:DataContext="{d:DesignInstance Type=vm:AppPage1ViewModel}"
      DataContext="{Binding Source={StaticResource Locator}, Path=AppPage1}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AppPage1">
    <Grid>
        <TextBox Text="{Binding Text}" Width="300" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left"/>
    </Grid>
</Page>
